{
  "name": "ai-sketch-platform",
  "version": "0.1.0",
  "private": true,
  "cloudflare": {
    "build": {
      "command": "npm run build:pages",
      "destination": "out"
    }
  },
  "scripts": {
    "dev": "cross-env NEXT_CONFIG_FILE=next.config.dev.mjs next dev",
    "build": "cross-env NEXT_CONFIG_FILE=next.config.vercel.mjs next build",
    "build:vercel": "cross-env NEXT_CONFIG_FILE=next.config.vercel.mjs next build",
    "download-gallery-images": "node scripts/download-gallery-images.js",
    "cache-hero-images": "node scripts/cache-hero-images.js",
    "build:pages": "rm -rf .next out && cross-env NEXT_CONFIG_FILE=next.config.static.mjs next build && mkdir -p out/_next/static && cp -r .next/static/* out/_next/static/ && cp -r .next/server/app/* out/ && cp -r public/* out/",
    "build:static": "cross-env NEXT_CONFIG_FILE=next.config.static.mjs next build && mkdir -p out && cp -r .next/server/app/* out/ && cp -r .next/static out/_next/ && cp -r public/images out/ && cp public/*.png public/*.jpg public/*.svg out/ 2>/dev/null || true",
    "build:workers": "wrangler deploy --config wrangler.workers.toml",
    "build:frontend": "echo 'Frontend embedded in Worker'",
    "start": "next start",
    "lint": "next lint",
    "deploy:vercel": "vercel --prod",
    "deploy:pages": "npm run build:pages && wrangler pages deploy out --project-name aimagica",
    "deploy:static": "npm run build:static && wrangler pages deploy out --project-name aimagica",
    "deploy:workers": "npm run build:workers",
    "dev:workers": "wrangler dev --config wrangler.workers.toml",
    "deploy:cloudflare": "npm run deploy:workers && npm run deploy:static",
    "preview:vercel": "vercel",
    "preview:pages": "npm run build:pages && wrangler pages dev out",
    "preview:static": "npm run build:static && wrangler pages dev out",
    "preview:workers": "wrangler dev",
    "verify:vercel": "node scripts/verify-vercel-deploy.js",
    "verify:cloudflare": "node scripts/verify-cloudflare-deploy.js",
    "tail:workers": "wrangler tail aimagica-api",
    "clean": "rm -rf .next out node_modules/.cache"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.1",
    "@aws-sdk/client-s3": "^3.826.0",
    "@aws-sdk/s3-request-presigner": "^3.826.0",
    "@emotion/is-prop-valid": "latest",
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-alert-dialog": "1.1.4",
    "@radix-ui/react-aspect-ratio": "1.1.1",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-checkbox": "1.1.3",
    "@radix-ui/react-collapsible": "1.1.2",
    "@radix-ui/react-context-menu": "2.2.4",
    "@radix-ui/react-dialog": "1.1.4",
    "@radix-ui/react-dropdown-menu": "2.1.4",
    "@radix-ui/react-hover-card": "1.1.4",
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-menubar": "1.1.4",
    "@radix-ui/react-navigation-menu": "1.2.3",
    "@radix-ui/react-popover": "1.1.4",
    "@radix-ui/react-progress": "1.1.1",
    "@radix-ui/react-radio-group": "1.2.2",
    "@radix-ui/react-scroll-area": "1.2.2",
    "@radix-ui/react-select": "2.1.4",
    "@radix-ui/react-separator": "1.1.1",
    "@radix-ui/react-slider": "1.2.2",
    "@radix-ui/react-slot": "1.1.1",
    "@radix-ui/react-switch": "1.1.2",
    "@radix-ui/react-tabs": "1.1.2",
    "@radix-ui/react-toast": "1.2.4",
    "@radix-ui/react-toggle": "1.1.1",
    "@radix-ui/react-toggle-group": "1.1.1",
    "@radix-ui/react-tooltip": "1.1.6",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.0",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "date-fns": "^3.6.0",
    "dotenv": "^16.5.0",
    "embla-carousel-react": "8.5.1",
