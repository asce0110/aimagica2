    "@types/react-dom": "^18",
    "cross-env": "^7.0.3",
    "eslint": "^9.30.1",
    "postcss": "^8.5",
    "tailwindcss": "^3.4.17",
    "typescript": "^5",
    "vercel": "^44.2.7",
    "wrangler": "^3.102.0"
  }
}
